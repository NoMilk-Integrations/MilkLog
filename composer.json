{
  "name": "rootaccessplease/milklog",
  "description": "A Laravel package for unified logging and Slack notifications with fluent API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dennis",
      "email": "da@nomilk.dk"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/log": "^10.0|^11.0",
    "illuminate/notifications": "^10.0|^11.0",
    "laravel/slack-notification-channel": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "RootAccessPlease\\MilkLog\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RootAccessPlease\\MilkLog\\MilkLogServiceProvider"
      ],
      "aliases": {
        "MilkLog": "RootAccessPlease\\MilkLog\\Facades\\MilkLog"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
